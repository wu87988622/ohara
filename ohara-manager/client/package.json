{
  "name": "ohara-manager-client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^10.16.3"
  },
  "scripts": {
    "setup": "yarn && yarn build",
    "start": "react-scripts start",
    "build": "CI=true react-scripts build",
    "lint": "eslint --max-warnings 0 src cypress",
    "lint:ci": "CI=true eslint --max-warnings 0 src cypress",
    "vet": "yarn audit",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "yarn test",
    "test:report": "CI=true yarn test --reporters=default --reporters=jest-junit",
    "test:coverage": "yarn test --coverage --watchAll=false",
    "test:api:open": "cypress open --config integrationFolder=cypress/api",
    "test:api:run": "cypress run --config integrationFolder=cypress/api",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run"
  },
  "dependencies": {
    "@material-ui/core": "4.3.3",
    "@material-ui/icons": "4.2.1",
    "@testing-library/cypress": "4.1.1",
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "8.0.7",
    "classnames": "2.2.6",
    "cypress": "3.4.1",
    "cypress-plugin-retries": "1.2.2",
    "d3v4": "4.2.2",
    "dagre-d3": "0.6.1",
    "deep-diff": "1.0.2",
    "eslint-plugin-cypress": "2.6.1",
    "final-form": "4.18.5",
    "jest-junit": "8.0.0",
    "moment": "2.24.0",
    "react": "16.8.4",
    "react-content-loader": "4.2.2",
    "react-document-title": "2.0.3",
    "react-dom": "16.8.4",
    "react-draggable": "3.3.2",
    "react-final-form": "6.3.0",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.1",
    "styled-components": "4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended"
    ],
    "plugins": [
      "jest"
    ],
    "globals": {
      "cy": false,
      "Cypress": false,
      "describe": false,
      "context": false,
      "before": false,
      "after": false,
      "beforeEach": false,
      "afterEach": false,
      "it": false,
      "assert": false,
      "expect": false
    },
    "rules": {
      "jest/no-focused-tests": "warn",
      "jest/valid-describe": "warn",
      "jest/no-identical-title": "warn",
      "no-alert": "warn",
      "no-template-curly-in-string": "warn",
      "array-callback-return": "warn",
      "object-shorthand": "warn",
      "no-console": "warn",
      "no-unused-vars": "warn",
      "no-debugger": "warn",
      "react/prop-types": "warn",
      "no-case-declarations": "off"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
